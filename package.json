{
  "name": "@anycli/dev-cli",
  "version": "0.1.3",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./lib/commands",
    "bin": "anycli-dev",
    "plugins": [
      "@anycli/plugin-help",
      "@anycli/plugin-not-found",
      "@anycli/plugin-version"
    ]
  },
  "bin": {
    "anycli-dev": "./bin/run"
  },
  "bugs": "https://github.com/anycli/dev-cli/issues",
  "dependencies": {
    "@anycli/command": "^0.3.7",
    "@anycli/config": "^0.3.0",
    "@anycli/engine": "^0.3.0",
    "@anycli/plugin-help": "^0.4.0",
    "@anycli/plugin-not-found": "^0.1.10",
    "@anycli/plugin-version": "^0.1.20",
    "cli-ux": "^3.3.13",
    "fs-extra": "^5.0.0"
  },
  "devDependencies": {
    "@anycli/test": "^0.10.3",
    "@anycli/tslint": "^0.2.2",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.0",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/node-notifier": "^0.0.28",
    "@types/read-pkg": "^3.0.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.16.0",
    "eslint-config-anycli": "^1.3.1",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/anycli/dev-cli",
  "keywords": [
    "anycli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "anycli/dev-cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"eslint .\" \"tsc -p test --noEmit\" \"tslint -p test\"",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
